{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/*",
            "allowedRoles": [
                "reader"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/aad"
        }
    },
    "auth": {
        "identityProviders": {
            "github": {
                "registration": {
                  "clientIdSettingName": "GITHUB_CLIENT_ID",
                  "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
                }
            }
        }
    }
}